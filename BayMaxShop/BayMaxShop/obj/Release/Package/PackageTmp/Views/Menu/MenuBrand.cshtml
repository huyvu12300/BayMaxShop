@model IEnumerable<BayMaxShop.Models.EF.Brand>

@if (Model != null && Model.Any())
{
    <ul class="sidebar_categories">
        @foreach (var item in Model)
        {
            if (ViewBag.BrId == item.Id)

            {
                <li class="active"><a href="/thuong-hieu/@item.Alias-@(item.Id)"><span><i class="fa fa-angle-double-right" aria-hidden="true"></i></span>@item.BrandName</a></li>
            }
            else
            {
                <li><a href="/thuong-hieu/@item.Alias-@(item.Id)">@item.BrandName</a></li>
            }

        }
    </ul>
}


